{
	"name": "Celeste Dark",
	"colors": {
		"editor.background": "#222",
		"editor.foreground": "#fdfdfd",
		"activityBarBadge.background": "#ffb84d",
		"sideBarTitle.foreground": "#999"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#999"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#a68cd9"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#fdfdfd"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution",
				"keyword.operator",
				"constant.other.option.dash.shell",
				"string.unquoted.argument.dash",
				"constant.character",
				"constant.escape"
			],
			"settings": {
				"foreground": "#777"
			}
		},
		{
			"name": "Control Keyword",
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#fdfdfd"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#4db3ff"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#54dea7"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#f4bebe"
			}
		},
		{
			"name": "Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#fdfdfd"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"punctuation.definition.string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#d2bfac"
			}
		},
		{
			"name": "String Argument",
			"scope": [
				"constant.other.option"
			],
			"settings": {
				"foreground": "#e4d9cd"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#4dd2c3"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#ffb84d"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#d2bfac"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#222"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#222"
			}
		},
		{
			"name": "Markup - List",
			"scope": [
				"markup.list punctuation.definition.list_item"
			],
			"settings": {
				"foreground": "#ff944d"
			}
		},
		{
			"name": "Markup - Raw Block",
			"scope": [
				"markup.raw.block"
			],
			"settings": {
				"background": "#202020"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"function.declaration": "#f7a76e",
		"function.defaultLibrary": "#3ca7dd",
		"variable": "#7d86d9",
		"variable.defaultLibrary": "#5fd9dd",
	}
}
